---
layout: layouts/base.njk
---

<article class="post post-enter">
  <header class="post-head">
    <h2 class="post-section-title">thinking out loud</h2>
    <h1 class="post-title">{{ title }}</h1>
    {% if date %}<p class="post-meta">{{ date | readableDate }}</p>{% endif %}
  </header>

  <div class="post-content">
    {{ content | safe }}
  </div>
</article>
